{%- capture json -%}
[
  {%- for item in site.inmuebles -%}
  {
    "title": {{ item.title | jsonify}},
    "status": {{ item.status | jsonify}},
    "city": {{ item.address.city | json}},
    "precio": {{ item.price }}
  }{%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
]
{%- endcapture -%}
{{ json | strip }}
